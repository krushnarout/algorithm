# URL GitHub will send users to after login. Replace "localhost" with your domain if not running locally.
GITHUB_REDIRECT_URI=http://localhost:3000/auth/github/callback

# Base URL of your API. Replace "localhost" with your domain if not running locally.
API_BASE_URL=http://localhost:3000

# Client ID for your GitHub OAuth app. Get this from GitHub's Developer settings: https://github.com/settings/developers
GITHUB_CLIENT_ID=your_github_client_id

# Client Secret for your GitHub OAuth app. Get this from GitHub's Developer settings: https://github.com/settings/developers
GITHUB_CLIENT_SECRET=your_github_client_secret

# Port number your server will run on.
PORT=3000

# Connection string for your MongoDB database.
MONOGO_URI=mongodb://localhost:27017/your_db_name
